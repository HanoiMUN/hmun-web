{
  "scripts": {
    "clean": "run-script-os",
    "clean:darwin:linux": "rm -rf ./public",
    "clean:win32": "rmdir /s .\\public",
    "format": "npx rustywind --write .",
    "postcss": "run-script-os",
    "postcss:darwin:linux": "npx postcss ./css/*.css -d ./public",
    "postcss-watch": "run-script-os",
    "postcss-watch:darwin:linux": "npx postcss ./css/*.css -d ./public -w",
    "build": "run-script-os",
    "build:darwin:linux": "npm run postcss && npx zola-bin build",
    "build:win32": "npm run postcss & npx zola-bin build",
    "serve": "npx zola-bin serve -O",
    "dev": "run-script-os",
    "dev:darwin:linux": "npm run postcss-watch & npm run serve",
    "dev:win32": "start /b npm run postcss-watch & start /b npm run serve"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.18",
    "postcss": "^8.4.35",
    "postcss-cli": "^11.0.0",
    "postcss-extend": "^1.0.5",
    "run-script-os": "^1.1.6",
    "rustywind": "^0.21.0",
    "tailwindcss": "^3.4.1",
    "zola-bin": "^0.3.7"
  }
}
